{
  "tabWidth": 2,
  "useTabs": true,
  "singleQuote": false,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "trailingComma": "all",
  "bracketSameLine": true,
  "printWidth": 80,
  "linebreak-style": [true, "LF"],
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    }
  ]
}
